// $XFree86: xc/programs/xkbcomp/types/numpad,v 1.1 2003/08/09 14:30:49 pascal Exp $

xkb_types "microsoft" {

    type "KEYPAD" {
        modifiers = Shift+NumLock;
        map[None] = Level1;
        preserve[Shift] = Shift;
        map[NumLock] = Level2;
        level_name[Level1] = "Base";
        level_name[Level2] = "Number";
    };
};
